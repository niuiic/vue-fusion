{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build#build": {},
    "builtins#build": {},
    "styles#build": {},
    "components#build": {
      "dependsOn": ["build#build", "builtins#build", "styles#build"]
    },
    "components#build:doc": {
      "dependsOn": ["build#build", "builtins#build", "styles#build"]
    },
    "components#dev": {
      "dependsOn": ["build#build", "builtins#build", "styles#build"]
    },
    "website#build": {
      "dependsOn": ["build#build", "builtins#build", "styles#build", "components#build"]
    },
    "website#dev": {
      "dependsOn": ["build#build", "builtins#build", "styles#build", "components#build"]
    }
  }
}
