{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build#build": {},
    "components#build": {
      "dependsOn": ["build#build"]
    },
    "components#build:doc": {
      "dependsOn": ["build#build"]
    },
    "components#dev": {
      "dependsOn": ["build#build"],
      "persistent": true
    },
    "website#build": {
      "dependsOn": ["build#build", "components#build"]
    },
    "website#dev": {
      "dependsOn": ["build#build", "components#build"],
      "persistent": true
    },
    "website#dev:mock": {
      "dependsOn": ["build#build", "components#build"],
      "persistent": true
    }
  }
}
