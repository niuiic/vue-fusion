{
  "name": "website",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:mock": "vite --host --mode devmock",
    "build": "vite build",
    "build:mock": "vite build --mode buildmock",
    "lint:ts": "vue-tsc --noEmit",
    "lint:eslint": "eslint -f unix --ext .ts,vue .",
    "lint:stylelint": "stylelint -f unix \"**/*.{css,scss,vue}\"",
    "debug:cmd": "vite build"
  },
  "dependencies": {
    "components": "workspace:*",
    "fetch": "workspace:*",
    "mock": "workspace:*",
    "utils": "workspace:*",
    "hooks": "workspace:*",
    "business": "workspace:*",
    "styles": "workspace:*",
    "router": "workspace:*"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "stylelint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "postcss-plugins": "workspace:*",
    "vite-plugins": "workspace:*",
    "types": "workspace:*"
  }
}
